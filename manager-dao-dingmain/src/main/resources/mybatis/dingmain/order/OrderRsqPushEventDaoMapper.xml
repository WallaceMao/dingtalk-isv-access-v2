<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rishiqing.dingtalk.mgr.dingmain.dao.mapper.order.OrderRsqPushEventDao">

    <resultMap type="com.rishiqing.dingtalk.api.model.domain.order.OrderRsqPushEventDO" id="orderRsqPushEventMap">
        <id property="id" column="id" />
        <result property="gmtCreate" column="gmt_create" />
        <result property="gmtModified" column="gmt_modified" />
        <result property="suiteKey" column="suite_key" />
        <result property="orderId" column="order_id" />
        <result property="corpId" column="corp_id" />
        <result property="quantity" column="quantity" />
        <result property="serviceStopTime" column="service_stop_time" />
        <result property="rsqTeamId" column="rsq_team_id" />
        <result property="status" column="status" />
    </resultMap>

    <insert id="saveOrUpdateOrderRsqPushEvent" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO isv_order_rsq_push_event (
        gmt_create,
        gmt_modified,
        suite_key,
        order_id,
        corp_id,
        quantity,
        service_stop_time,
        rsq_team_id,
        `status`
        )VALUES(
        now(),
        now(),
        #{suiteKey},
        #{orderId},
        #{corpId},
        #{quantity},
        #{serviceStopTime},
        #{rsqTeamId},
        #{status}
        )ON DUPLICATE KEY UPDATE gmt_modified=now(), `status`=#{status}
    </insert>

</mapper>